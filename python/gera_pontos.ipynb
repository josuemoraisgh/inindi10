      "source": [
        "import numpy as np\n",
        "x = np.matrix(np.arange(0,20,1)+np.random.uniform(0,1,20))\n",
        "a,b,c,d = 30,53,23,32\n",
        "y = np.matrix((a*np.power(x,3)+b*np.power(x,2)+c*x+d)+(np.random.uniform(0,1,20)))\n",
        "txt  = np.concatenate((x.T,y.T),1)\n",
        "np.savetxt('dados.txt', txt, delimiter=',')"
      ]
    }
  ],
